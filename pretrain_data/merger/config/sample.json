{
  "streams": [
    {
      "name": "cc_en_head",
      "format": "ai2",
      "documents" : {
        "root": "s3://ai2-llm/pretraining-data/sources/common-crawl/v0/documents/",
        "include": ["*/*/*/en_head.json.gz"]
      },
      "attributes": {
        "root": "s3://ai2-llm/pretraining-data/sources/common-crawl/v0/attributes",
        "include": ["toxicity"]
      },
      "sampler": {
        "seed": 117,
        "rate": 1.0
      },
      "filterer": {
        "include": [".metadata.length < 100000"],
        "exclude": [".metadata.length < 100"]
      }
    }
  ],
  "output": {
    "path": "/data/test",
    "max_file_size": "4G"
  },
  "processes": 32
}
